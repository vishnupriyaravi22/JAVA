import java.io.*;
import java.util.Scanner;
 
public class Main {
  public static void main(String[] args) {
 
  int a, b, result;
 
  System.out.println("Input two integers");
  Scanner s = new Scanner(System.in);
  a = s.nextInt();
  b = s.nextInt();
 
  result = a / b;
 
  System.out.println("Result = " + result);
  }
}
